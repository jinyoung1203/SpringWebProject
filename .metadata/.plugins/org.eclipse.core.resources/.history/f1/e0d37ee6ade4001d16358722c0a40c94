<mapper namespace="b">
	<select id="preorder_list_product" resultType="product">
		SELECT *
		FROM product
		WHERE product_idx IN
		<foreach item="index" collection="preorder_idx" separator=","
			open="(" close=")">
			#{index}
		</foreach>
		ORDER BY producer_idx ASC;
	</select>
	<select id="producerName" resultType="producer">
	</select>
